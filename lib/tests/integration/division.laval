; Perform an integer division such as: return = a / b

.cores 1, 2, 2
.mem_number 9
.mem_size 7
.mem_map 0, 3, 7, 5
.in 0, 1


; Subtractor
0:
    MUX CURRENT, BEFORE, CURRENT
    MXL
    MUX CURRENT, CURRENT, AFTER
    JMP 1

1:
    JEZ 2
    JLZ 2
    MXS
    JMP 1

2:
    SYN


; Send operand
3:
    MUX CURRENT, BEFORE, CURRENT
    MXL
    MUX CURRENT, AFTER, CURRENT
    JMP 4

4:
    SYN
    MXD
    JMP 4


; Counter
5:
    MUX CURRENT, BEFORE, CURRENT
    JMP 6

6:
    SYN
    CAD 1
    JMP 6

; Check when its time to halt
7:
    MUX CURRENT, BEFORE, CURRENT
    MXL
    JEZ 8
    MUX CURRENT, CURRENT, AFTER
    MXL
    CSU 1
    HLT

8:
    MUX CURRENT, CURRENT, AFTER
    MXL
    HLT
